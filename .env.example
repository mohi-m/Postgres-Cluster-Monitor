# Copy this file to .env and fill values before running `docker-compose up`
# Do NOT commit real credentials to source control.

# Database credentials (used by backend and pgbouncer templates)
DB_USER=postgres
DB_PASSWORD=your_db_password
DB_NAME=your_database_name

# Azure Postgres node IPs (fill with your cluster IPs)
PRIMARY_HOST=<PRIMARY_PUBLIC_IP>
SECONDARY1_HOST=<SECONDARY1_PUBLIC_IP>
SECONDARY2_HOST=<SECONDARY2_PUBLIC_IP>

# pgbouncer exposed port
PGBOUNCER_PORT=6432

# Backend/Locust settings
BACKEND_PORT=8000
